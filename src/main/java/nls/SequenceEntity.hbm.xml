<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="nls.SequenceEntity" table="sequence" schema="bydb">
        <id name="seqName">
            <column name="SEQ_NAME" sql-type="varchar(50)" length="50"/>
        </id>
        <property name="seqCount">
            <column name="SEQ_COUNT" sql-type="decimal(38)" precision="38" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>